#include <stdio.h>  //Write a program to take a string as an input from the user using %c and %s. Confirm that the strings are equal.

int main() {
    char name1[15],name2[20];
    int i;
    
    for(i=0;i<15;i++){
    scanf("%c",&name1[i]);
    }
    printf("%s",name1);
    
    for(i=0;i<20;i++){
        scanf("%s",&name2);
    }
    printf("%s",name2);

    return 0;
}
