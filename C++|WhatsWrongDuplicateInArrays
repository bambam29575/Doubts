#include <iostream>
using namespace std;

int main(){
    int arr1[4] = {1,2,3,4}, arr2[9] = {9,8,7,6,5,4,3,2}, arr3[4];
        for(int k = 0; k < 4; k++){
           for(int i = 0; i < 9; i++){
              if(arr1[k] == arr2[i]){
                 arr3[k] = arr1[k];
                } 
            }
        }
    for(int l = 0; l < 4; l++){
        if(arr3[l] == 0){
                break;
            
        }else{
             cout << arr3[l] << " ";
            }
            
    }
    
    //cout<< "All is well";
    return 0;
}
